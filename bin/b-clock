#!/bin/sh

clock=$(date '+%I')

case "$clock" in
  "01") icon_clock="  " ;;
  "02") icon_clock="  " ;;
  "03") icon_clock="  " ;;
  "04") icon_clock="  " ;;
  "05") icon_clock="  " ;;
  "06") icon_clock="  " ;;
  "07") icon_clock="  " ;;
  "08") icon_clock="  " ;;
  "09") icon_clock="  " ;;
  "10") icon_clock="  " ;;
  "11") icon_clock="  " ;;
  "12") icon_clock="  " ;;
esac

date "+$icon_clock%I:%M%p"
